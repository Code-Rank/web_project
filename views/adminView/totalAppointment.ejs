<!DOCTYPE html>
<html lang="en">
  <head>
    <meta name="viewpoint" content="width=device-width, initial-value=1.0" />
    <link rel="stylesheet" type="text/css" href="/assets/css/style.css" />
    <link
      rel="shortcut-icon"
      width="16px"
      height="16px"
      href="assets/images/sadapay-logo.webp"
    />
    <link
      rel="stylesheet"
      type="text/css"
      href="/assets/css/bootstrap.min.css"
    />
    <link rel="stylesheet" type="text/css" href="/assets/css/all.min.css" />
    <title>sadapay</title>
  </head>

  <div>
    <!--Navbar-->
    <%- include('../partial/navbar.ejs') %>

    <!--Admin-->

    <div class="container mt-5">
      <div class="row">
      <%- include('../partial/sidebar.ejs') %>
      <!--Appointment and bills-->
      <div class="col-8">
        <!--Cards-->

        <div class="row">
          <div class="col-md-5">
            <div
              class="card text-white bg-dark mb-3"
              style="max-width: 20rem"
            >
              <div class="card-header">Total Doctor</div>
              <div class="card-body">
                <h5 class="card-title">Doctor</h5>
                <p class="card-text"><%- totalDoctors %></p>
              </div>
            </div>
          </div>
          <div class="col-md-5">
            <div
              class="card text-white bg-warning mb-3"
              style="max-width: 20rem"
            >
              <div class="card-header">Total Patients</div>
              <div class="card-body">
                <h5 class="card-title">Patients</h5>
                <p class="card-text">1231,123</p>
              </div>
            </div>
          </div>
          <div class="col-md-5">
            <div
              class="card text-white bg-primary mb-3"
              style="max-width: 20rem"
            >
              <div class="card-header">Total Appointments</div>
              <div class="card-body">
                <h5 class="card-title">Appointmemnt</h5>
                <p class="card-text">1231,123</p>
              </div>
            </div>
          </div>
          <div class="col-md-5">
            <div
              class="card text-white bg-info mb-3"
              style="max-width: 20rem"
            >
              <div class="card-header">Payments</div>
              <div class="card-body">
                <h5 class="card-title">Total Doctors</h5>
                <p class="card-text">1231,123</p>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
       
		    
        
      </div>
    </div>
	</div>

    <script src="assets/js/jquery.min.js"></script>
    <script src="assets/js/bootstrap.min.js"></script>
  </body>
</html>
